<%args>
$target 
</%args>
<!-- iniciation of JS for calendar(s) --!>
<script type="text/javascript" src="/javascripts/jsDatePick.min.1.3.js"></script>
<script type="text/javascript">
	window.onload = function(){
		new JsDatePick({
			useMode:2,
			target:"<% $target %>",
			dateFormat:"%j.%n.%Y",
			cellColorScheme:"beige",
			weekStartDay:1
		});
	};
</script>
